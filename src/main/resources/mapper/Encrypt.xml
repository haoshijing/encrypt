<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hulian.encrypt.dao.EncryptDao">

    <sql id="tableName">
        t_encrypt_data
    </sql>
    <!-- 插入一条记录 -->
    <insert id="insert" parameterType="com.hulian.encrypt.po.EncryptPo">
        INSERT INTO <include refid="tableName"/>
        (
        hardId,
        licence,
        createTime
        )
        VALUES (
        #{hardId},
        #{licence},
        #{createTime}
        )
    </insert>

    <select id = "selectByHardId" parameterType="java.lang.String" resultType="com.hulian.encrypt.po.EncryptPo" >
        select * from  <include refid="tableName"/>
        where hardId = #{hardId}
    </select>

</mapper>